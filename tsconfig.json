{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "build",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "typeRoots": ["./src/@types", "node_modules/@types"]
  },
  "lib": ["es2015"],
  "include": ["src"],
  "exclude": ["src/handlers/quotes_old"]
}
